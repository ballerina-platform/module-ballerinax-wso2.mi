{
  "connectorName": "{{parent.name}}",
  "operationName": "{{name}}",
  "title": "CSV to Json",
  "help": "<h1>CSV to JSON</h1> <b>Convert a CSV payload to JSON</b>",
  "elements": [
    {
      "type": "attributeGroup",
      "value": {
        "groupName": "General",
        "elements": [
          {
            "type": "attribute",
            "value": {
              "name": "description",
              "displayName": "Description",
              "inputType": "string",
              "defaultValue": "Ballerina Connector",
              "required": "false",
              "helpTip": "Component description"
            }
          },{
            "type": "attributeGroup",
            "value": {
              "groupName": "Target",
              "elements": [
                {
                  "type": "attribute",
                  "value": {
                    "name": "Result",
                    "displayName": "Target",
                    "inputType": "string",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Result of the operation to be stored in a property"
                  }
                }
              ]
            }},
          {
            "type": "attributeGroup",
            "value": {
              "groupName": "Variables",
              "elements": [
                {{#each balFuncParams ~}}
                {
                  "type": "attribute",
                  "value": {
                    "name": "{{name}}",
                    "displayName": "{{name}} [xml]",
                    "inputType": "string",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "Parameter Value (OMElement)"
                  }
                },
                {{/each ~}}
              ]
            }
          }
        ]
      }
    }
  ]
}
